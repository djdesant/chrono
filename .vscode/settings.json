{
    "cmake.useCMakePresets": "never",
    // "workbench.editorAssociations": {   
    //     "*.md": "vscode.markdown.preview.editor",
    // },

    //*** Live Server *** ///
    "liveServer.settings.multiRootWorkspaceName": "",
    "liveServer.settings.port": 5500,
    "liveServer.settings.CustomBrowser": "firefox",
    "liveServer.settings.useWebExt": true,
    "liveServer.settings.root": "/",
    "liveServer.settings.NoBrowser" : false,
    
    "liveServer.settings.ignoreFiles" : [
        ".vscode/**",
        // "**/*.scss",
        // "**/*.sass"
    ],


    "terminal.integrated.gpuAcceleration": "on",
    "gitlens.codeLens.enabled": false,
    /*** Editor Settings **/
    "editor.autoIndent": "full",
    "editor.formatOnType": false,
    "editor.detectIndentation": true,
    // "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "gitlens.plusFeatures.enabled": false, 
    "[git-commit]": {
        "editor.rulers": [50, 72],
    },
    "editor.quickSuggestions": {
        "other": true,
        "comments": false,
        "strings": false
    },
    "editor.rulers": [120],
    "editor.semanticHighlighting.enabled": true,
    "terminal.integrated.cwd": "${workspaceFolder}",
    "C_Cpp.enhancedColorization": "enabled",
    "C_Cpp.loggingLevel": "Information",
    "C_Cpp.intelliSenseEngine": "default",
    "C_Cpp.errorSquiggles": "enabled",
    "C_Cpp.exclusionPolicy": "checkFilesAndFolders",
    "C_Cpp.caseSensitiveFileSupport": "enabled",
    // "C_Cpp.codeAnalysis.exclude": {
    //     // "**/Platform/OS/**": true,
    //     // "**/Application/**": true,
    //     "${workspaceFolder}/ThirdParty/STW_Platform/KMC300/c_bios_safety_core/x_intrinsics.h": true,
    //     "/opt/krone_eu/Hightec/toolchains/tricore/v4.9.4.0/bin/tricore-gcc/include/**": true,
    // },
    // "C_Cpp.files.exclude": {
    //     // "**/Platform/OS/**": true,
    //     // "**/Application/**": true,
    //     "${workspaceFolder}/ThirdParty/STW_Platform/KMC300/c_bios_safety_core/x_intrinsics.h": true,
    //     "/opt/krone_eu/Hightec/toolchains/tricore/v4.9.4.0/bin/tricore-gcc/include/**": true,
    // },
    "C_Cpp.codeAnalysis.runAutomatically": true,
    "C_Cpp.codeAnalysis.clangTidy.enabled": true,
    // "C_Cpp.codeAnalysis.clangTidy.headerFilter": "^${workspaceFolder}/Application/.*",
    "C_Cpp.codeAnalysis.clangTidy.fallbackConfig": "",
    "C_Cpp.codeAnalysis.exclude": {
        "**/ThirdParty*": true,
        "**/Platform*": true,
        "**/Test/**": true,
        "**/_build": true
    },
    "C_Cpp.files.exclude": {
        // "**/ThirdParty": true,
        // "**/Platform": true
        "**/.vscode": true,
        "**/.ude": true,
        "**/.settings": true,
        "**/_build": true
    },
    // "C_Cpp.codeAnalysis.clangTidy.path": "/usr/bin/clang-tidy",
    "C_Cpp.codeAnalysis.clangTidy.args": [
        // "--extra-arg=-ferror-limit=1",
        // "--extra-arg=-mfloat-abi=hh32",
        // "--extra-arg=-target=arm-zephyr-eabi-gcc",
        // "--extra-arg=-march=x86",
        // "--extra-arg=-mfpu=neon",
        // "--extra-arg=-mfloat-abi=hard",
        // "-p","${workspaceFolder}/compile_commands.json",
        "--extra-arg=-std:c++17",
        "--extra-arg=-CHRONO_DATA_DIR={workspaceFolder}/build/data",
        // "--extra-arg=-m32",
        "--extra-arg=-Wall",
        "--extra-arg=-Wextra",
        "--extra-arg=-Werror",
        // "--extra-arg=-Wmissing-field-initializers",
        // "--extra-arg-before=-target=arm32",
        // "--extra-arg=-march=zephyr",
        // "--extra-arg=-target=arm32",
        // "-mcpu=tc162", // AURIXâ„¢ platform 2nd generation (TC3xx)
        // "--extra-arg=-mfpu", // FPU 32bit
        // "--extra-arg=-mfloat-abi=hh32", // FPU 32bit
        // "--system-headers"
        //  "--extra-arg=-fno-exceptions",
        //  "--exclude-header-filter=${workspaceFolder}/ThirdParty/STW_Platform/KMC75/c_bios_safety_core/.*"
    ],
    // "C_Cpp.codeAnalysis.clangTidy.args": [
    //     "--config-file=./.vscode/.clang-tidy"
    // ],
    "C_Cpp.codeAnalysis.clangTidy.checks.enabled": [
        "bugprone-*",
        "clang-analyzer-*",
        "cppcoreguidelines-*",
        // "google-*",
        "hicpp-*",
        "llvm-*",
        "misc-*",
        "modernize-*",
        "readability-*",
        "performance-*",
    ],
    "C_Cpp.codeAnalysis.clangTidy.checks.disabled": [
        "bugprone-branch-clone",
        "bugprone-easily-swappable-parameters",
        "cppcoreguidelines-pro-type-member-init",
        "cppcoreguidelines-special-member-functions",
        "cppcoreguidelines-pro-bounds-constant-array-index",
        "cppcoreguidelines-pro-bounds-array-to-pointer-decay",
        "cppcoreguidelines-prefer-member-initializer",
        "cppcoreguidelines-avoid-magic-numbers",
        "cppcoreguidelines-explicit-virtual-functions",
        "cppcoreguidelines-use-default-member-init",
        "cppcoreguidelines-avoid-c-arrays",
        "cppcoreguidelines-avoid-const-or-ref-data-members",
        "readability-avoid-const-params-in-decls",
        "readability-implicit-bool-conversion",
        "readability-simplify-boolean-expr",
        "readability-redundant-member-init",
        "readability-magic-numbers",
        "readability-function-cognitive-complexity",
        "readability-convert-member-functions-to-static",
        "readability-use-std-min-max",
        "readability-const-return-type",
        "readability-identifier-length",
        "performance-enum-size",
        "modernize-deprecated-headers",
        "modernize-use-equals-default",
        "modernize-use-equals-delete",
        "modernize-use-using",
        "modernize-use-auto",
        "modernize-use-override",
        "modernize-loop-convert",
        "modernize-use-default-member-init",
        "modernize-redundant-void-arg",
        "modernize-use-trailing-return-type",
        "modernize-avoid-c-arrays",
        "hicpp-use-auto",
        "cert-dcl58-cpp",
        "hicpp-member-init",
        "hicpp-special-member-functions",
        "hicpp-use-equals-default",
        "hicpp-use-override",
        "hicpp-explicit-conversions",
        "hicpp-avoid-c-arrays",
        "llvm-include-order",
        "llvm-header-guard",
        "misc-include-cleaner"
        // "google-runtime-*"
    ],
